{
  "name": "oxidio/project",
  "type": "project",
  "license": "GPL-3.0",
  "repositories": [
    {"type":  "path", "url": "resources/*", "options": {"symlink": true}}
  ],
  "require": {
    "oxidio/bin": "@dev",
    "oxidio/composer-plugin": "@dev",
    "oxidio/doctrine-migration-wrapper": "@dev",
    "oxidio/facts": "@dev",
    "oxidio/module-klarna": "@dev",
    "oxidio/module-paypal": "@dev",
    "oxidio/module-paypalplus": "@dev",
    "oxidio/module-seo": "@dev",
    "oxidio/oxidio": "@dev",
    "oxidio/shop": "@dev",
    "oxidio/theme-flow": "@dev",
    "oxidio/theme-wave": "@dev",
    "oxidio/unified-namespace-generator": "@dev",

    "php": ">=7.1"
  },
  "require-dev": {
    "composer/composer": "^1.0",
    "oxidio/test": "@dev"
  },
  "autoload": {
    "psr-4": {
      "OxidEsales\\EshopCommunity\\MigrationsProject\\": [
        "../../../source/migration/project_data/",
        "source/migration/project_data/"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  }
}
