{
  "name": "oxidio/project",
  "type": "project",
  "license": "GPL-3.0",
  "repositories": [
    {"type":  "path", "url": "resources/*", "options": {"symlink": true}}
  ],
  "require": {
    "oxidio/bin": "dev-master",
    "oxidio/composer-plugin": "dev-master",
    "oxidio/doctrine-migration-wrapper": "dev-master",
    "oxidio/facts": "dev-master",
    "oxidio/module-seo": "dev-master",
    "oxidio/oxidio": "dev-master",
    "oxidio/shop": "dev-master",
    "oxidio/theme-flow": "dev-master",
    "oxidio/theme-wave": "dev-master",
    "oxidio/unified-namespace-generator": "dev-master",

    "oxidio/module-klarna": "dev-master",
    "oxidio/module-paypal": "dev-master",
    "oxidio/module-paypalplus": "dev-master",

    "php": ">=7.1"
  },
  "require-dev": {
    "composer/composer": "^1.0",
    "oxidio/test": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "OxidEsales\\EshopCommunity\\MigrationsProject\\": [
        "../../../source/migration/project_data/",
        "source/migration/project_data/"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  }
}
