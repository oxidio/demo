{
  "type": "project",
  "name": "oxidio/project",
  "license": "GPL-3.0",
  "prefer-stable": true,
  "require": {
    "vlucas/phpdotenv": "^3",
    "phpdocumentor/reflection-docblock": "^4",
    "php-fn/php-fn": "^2",
    "php-fn/di": "^2",
    "php-fn/cli": "^2",
    "oxidio/oxidio": "^1|dev-master",
    "oxidio/bin": "^1",
    "oxidio/shop": "^6",
    "oxidio/facts": "^2",
    "oxidio/unified-namespace-generator": "^2",
    "oxidio/composer-plugin": "^2",
    "oxidio/theme-flow": "^3",
    "oxidio/doctrine-migration-wrapper": "^2",
    "symfony/yaml": "^3",
    "php": ">=7.1"
